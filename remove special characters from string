#include <stdio.h>

int main()
{
    char str[100];
    char str1[100];
    printf("enter string: ");
    scanf("%s",str);
    int i=0,j=0;
    while(str[i]!='\0')
    {
        if(((str[i]>='a'&&str[i]<='z')||(str[i]>='A'&&str[i]<='Z')))
        {
            str1[j]=str[i];
            j++;
        }
        
        i++;
    }
    str1[j]='\0';
    printf("%s",str1);

    return 0;
}
